package day1.task1;
import java.util.Scanner;
public class pattern2 {
public static void main(String[] args) {
	int n;
	Scanner s = new Scanner(System.in);
	n = s.nextInt();
	int k = 1;
	for(int i = 0; i < n; i++) {
		for(int j = 0; j <= i; j++) {
			System.out.print(k+ " ");
			k++;
		}
		System.out.print("\n");
	}
	
}
}
